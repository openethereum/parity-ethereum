{
  "presets": ["es2017", "es2016", "es2015", "stage-0", "react"],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    "lodash"
  ],
  "retainLines": true,
  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    }
  }
}
