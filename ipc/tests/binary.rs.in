use ipc::*;
use std::mem;
use std::collections::VecDeque;

#[derive(Binary)]
pub enum Root {
	Top,
	Middle(u32, u64),
}

#[derive(Binary, PartialEq, Debug)]
pub struct DoubleRoot {
	pub x1: u32,
	pub x2: u64,
	pub x3: u32,
}

#[derive(Binary, PartialEq, Debug)]
pub struct ReferenceStruct<'a> {
	pub ref_data: &'a u64,
}
